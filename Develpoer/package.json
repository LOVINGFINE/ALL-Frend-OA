{
  "name": "develpoer-server",
  "version": "1.0.0",
  "main": "main.ts",
  "license": "MIT",
  "scripts": {
    "start": "nodemon",
    "test": "nodemon ./__test__/index.ts"
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules",
      "public/font-unicode.json",
      "public/.development/*"
    ],
    "ext": "js json ts tsx jsx",
    "execMap": {
      "ts": "ts-node"
    },
    "watch": [
      "src",
      "public",
      "main.ts"
    ],
    "env": {
      "NODE_ENV": "development"
    },
    "legacy-watch": false
  },
  "dependencies": {
    "body-parser": "^1.19.1",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "moment": "^2.29.1",
    "svgtofont": "^3.14.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.10",
    "@types/yargs": "^17.0.8",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}